{
    "version": 2,
    "name": "express",
    "builds": [
        { "src": "app.js", "use": "@now/node" },
	{ "src": "routes/*.js", "use": "@now/node" },
	{ "src": "client/**/*.js", "use": "@now/next },
    ],
    "routes": [
	{ "src": "/", "dest": "/routes/index.js" },
	{ "src": "/app", "dest": "/routes/app.js" },
	{ "src": "/callback", "dest": "/routes/callback.js" },
    ],
}
